<!doctype html>
<html lang="en">
  <head>
    {% render 'shogun-optimizer' %}

    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {%- unless settings.favicon == blank -%}
      <link rel="shortcut icon" type="image/png" href="{{ settings.favicon | image_url: width: 32, height: 32 }}">
    {%- endunless -%}

    <title>
      {{ page_title }}
      {% if current_tags %} &ndash; tagged "{{ current_tags | join: ', ' }}"{% endif %}
      {% if current_page != 1 %} &ndash; Page {{ current_page }}{% endif %}
      {% unless page_title contains shop.name %} &ndash; {{ shop.name }}{% endunless %}
    </title>

    {% if page_description %}
      <meta name="description" content="{{ page_description | escape }}">
    {% endif %}

    <link rel="canonical" href="{{ canonical_url }}">

    {% comment %}
      Critical CSS
    {% endcomment %}
    <style>
      @font-face {
        font-family: 'Ordinary Regular';
        src: url('https://cdn.shopify.com/s/files/1/0898/1859/5696/files/ordinary-display-regular.woff2?v=1742796201')
          format('woff2');
        font-display: swap;
      }

      @font-face {
        font-family: 'Ordinary Light';
        src: url('https://cdn.shopify.com/s/files/1/0898/1859/5696/files/ordinary-display-light.woff2?v=1742796170')
          format('woff2');
        font-display: swap;
      }

      @font-face {
        font-family: 'The Wedding Signature';
        src: url('https://cdn.shopify.com/s/files/1/0898/1859/5696/files/the-wedding-signature-regular.woff2?v=1743742147')
          format('woff2');
        font-display: swap;
      }

      /* CSS Reset */

      *,
      *::before,
      *::after {
        box-sizing: border-box;
        margin: 0;
        padding: 0;
      }

      html:focus-within,
      html,
      body {
        scroll-behavior: smooth;
        height: 100%;
      }

      html {
        overflow-y: scroll;
      }

      ul[role='list'],
      ol[role='list'] {
        list-style: none;
      }

      li {
        list-style-position: inside;
      }

      a:not([class]) {
        text-decoration-skip-ink: auto;
      }

      img,
      picture,
      svg,
      video,
      canvas {
        max-width: 100%;
        height: auto;
        vertical-align: middle;
        font-style: italic;
        background-size: cover;
      }

      input,
      button,
      textarea,
      select {
        font: inherit;
      }

      @view-transition {
        navigation: auto;
      }

      @media (prefers-reduced-motion: reduce) {
        html:focus-within {
          scroll-behavior: auto;
        }
        *,
        *::before,
        *::after {
          animation-duration: 0.01ms !important;
          animation-iteration-count: 1 !important;
          transition-duration: 0.01ms !important;
          scroll-behavior: auto !important;
          transition: none;
        }
      }

      /* Variables */

      :root {
        /* Fluid Spacing */
        --space-3xs: clamp(0.25rem, 0.2375rem + 0.0625vw, 0.3125rem);
        --space-2xs: clamp(0.5rem, 0.475rem + 0.125vw, 0.625rem);
        --space-xs: clamp(0.75rem, 0.7125rem + 0.1875vw, 0.9375rem);
        --space-s: clamp(1rem, 0.95rem + 0.25vw, 1.25rem);
        --space-m: clamp(1.5rem, 1.425rem + 0.375vw, 1.875rem);
        --space-l: clamp(2rem, 1.9rem + 0.5vw, 2.5rem);
        --space-xl: clamp(3rem, 2.85rem + 0.75vw, 3.75rem);
        --space-2xl: clamp(4rem, 3.8rem + 1vw, 5rem);
        --space-3xl: clamp(6rem, 5.7rem + 1.5vw, 7.5rem);

        /* Colours */
        --white: #ffffff;
        --green-50: #eaf8f1;
        --green-100: #d4ede1;
        --green-200: #bae3cf;
        --green-300: #9ed8bc;
        --green-400: #48b381;
        --green-500: #398e64;
        --green-600: #2c6d4e;
        --green-700: #204e39;
        --green-800: #132c1f;
        --green-900: #0c1e15;
        --green-950: #06120b;
        --neutral-50: #f3f3f3;
        --neutral-100: #e7e7e7;
        --neutral-200: #dadada;
        --neutral-300: #cbcbcb;
        --neutral-400: #a0a0a0;
        --neutral-500: #7e7e7e;
        --neutral-600: #616161;
        --neutral-700: #444444;
        --neutral-800: #272727;
        --neutral-900: #191919;
        --neutral-950: #0f0f0f;
      }

      /* Accessibility */

      a.skip-to-content {
        position: absolute;
        top: 0;
        left: 0;
        transform: translateX(-100%);
      }

      a.skip-to-content:focus-visible {
        position: relative;
        transform: translateX(0%);
      }

      /* Typography */

      .small {
        line-height: 140%;
        letter-spacing: 0.065rem;
        font-family: 'Ordinary Light';
        font-size: clamp(0.5787rem, 0.5498rem + 0.1447vw, 0.7234rem);
      }

      .body,
      p,
      li {
        line-height: 140%;
        letter-spacing: 0.065rem;
        font-family: 'Ordinary Light';
        font-size: clamp(0.6944rem, 0.6597rem + 0.1736vw, 0.8681rem);
        font-weight: 300;
      }

      .body--uppercase {
        line-height: 140%;
        letter-spacing: 0.065rem;
        font-family: 'Ordinary Light';
        text-transform: uppercase;
        font-size: clamp(0.6944rem, 0.6597rem + 0.1736vw, 0.8681rem);
        font-weight: 300;
      }

      .body--bold,
      h5,
      h6 {
        line-height: 140%;
        letter-spacing: 0.065rem;
        font-family: 'Ordinary Regular';
        font-size: clamp(0.6944rem, 0.6597rem + 0.1736vw, 0.8681rem);
        font-weight: 500;
      }

      .heading--l,
      h4 {
        line-height: 140%;
        letter-spacing: 0.125rem;
        font-family: 'Ordinary Regular';
        font-size: clamp(0.8333rem, 0.7917rem + 0.2083vw, 1.0417rem);
        font-weight: 500;
        -moz-osx-font-smoothing: grayscale;
        -webkit-font-smoothing: antialiased;
      }

      .heading--xl,
      h3 {
        line-height: 140%;
        font-family: 'Ordinary Regular';
        font-size: clamp(1rem, 0.95rem + 0.25vw, 1.25rem);
        font-weight: 500;
      }

      .heading--2xl,
      h2 {
        line-height: 140%;
        letter-spacing: 0.125rem;
        font-family: 'Ordinary Regular';
        font-size: clamp(1.2rem, 1.14rem + 0.3vw, 1.5rem);
        font-weight: 500;
      }

      .heading--decorative {
        font-family: 'The Wedding Signature';
        line-height: 100%;
        font-size: clamp(2.4883rem, 2.3639rem + 0.6221vw, 3.1104rem);
        font-weight: 500;
        letter-spacing: 0 !important;
        -moz-osx-font-smoothing: grayscale;
        -webkit-font-smoothing: antialiased;
      }

      .heading--3xl,
      h1 {
        line-height: 100%;
        letter-spacing: 0.2rem;
        font-family: 'Ordinary Light';
        font-size: clamp(2.986rem, 2.8367rem + 0.7465vw, 3.7325rem);
        font-weight: 300;
      }

      .heading--4xl {
        line-height: 100%;
        letter-spacing: 0.2rem;
        font-family: 'Ordinary Regular';
        font-size: clamp(3.5832rem, 3.404rem + 0.8958vw, 4.479rem);
        font-weight: 500;
      }

      @media screen and (max-width: 768px) {
        .small,
        .body,
        .body--uppercase,
        .body--bold {
          font-size: 12px !important;
        }
      }

      /* Header */

      header {
        position: relative;
      }

      #site-header {
        width: 100%;
        z-index: 9996;
      }

      #header-spacer {
        height: var(--space-xl);
      }

      #site-header.header-fixed {
        position: fixed;
        top: 0;
        border-bottom: 1px solid var(--neutral-200);
      }

      #site-header.header-fixed.is-visible {
        top: 0;
      }

      .header {
        background-color: var(--white);
        display: flex;
        align-items: center;
        justify-content: space-between;
        height: var(--space-xl);
        padding: 0 var(--space-m);
      }

      .header__logo {
        display: flex;
        align-items: center;
        position: absolute;
        left: 50%;
        transform: translateX(-50%);
      }

      .header__icons {
        display: flex;
        list-style: none;
        gap: var(--space-s);
        align-items: center;
      }

      .hamburger-menu {
        background: none;
        border: none;
        cursor: pointer;
        padding: 0;
        display: flex;
        align-items: center;
        justify-content: center;
        width: 17px;
        height: 17px;
      }

      .hamburger-svg {
        display: block;
        width: 17px;
        height: 17px;
      }

      .hamburger-line {
        transition: all 0.3s ease;
        transform-origin: center;
        transform-box: fill-box;
      }

      .nav-open .top-line {
        transform: translate(0, 4px) rotate(45deg);
      }

      .nav-open .bottom-line {
        transform: translate(0, -4px) rotate(-45deg);
      }

      .cart-indicator {
        display: block;
        width: 8px;
        height: 8px;
        border-radius: 50%;
        background-color: #ff0000;
        position: absolute;
        bottom: -2px;
        right: -5px;
      }

      .header__search-desktop {
        display: none;
      }

      @media screen and (min-width: 769px) {
        .header__search-desktop {
          display: block;
          margin-top: 3px;
        }
      }

      @media (max-width: 768px) {
        .header__logo {
          width: 160px;
        }
      }
    </style>

    {{ 'theme.css' | asset_url | stylesheet_tag: media: 'print', onload="this.media='all'" }}

    {% comment %}
      Klaviyo Newsletter Image Preloading
    {% endcomment %}
    {%- unless settings.klaviyo_newsletter_image_1 == blank -%}
      <link rel="preload" as="image" href="{{ settings.klaviyo_newsletter_image_1 }}">
    {%- endunless -%}
    {%- unless settings.klaviyo_newsletter_image_2 == blank -%}
      <link rel="preload" as="image" href="{{ settings.klaviyo_newsletter_image_2 }}">
    {%- endunless -%}

    <script src="{{ 'theme.js' | asset_url }}" defer></script>
    <script src="{{ 'alpine.min.js' | asset_url }}" defer></script>
    <script src="{{ 'predictive-search.js' | asset_url }}" defer></script>
    <script src="{{ 'splide.min.js' | asset_url }}" defer></script>
    <script src="{{ 'ploiu-vanilla-js-accordion.min.js' | asset_url }}" defer></script>
    <script src="{{ 'quicklink.umd.js' | asset_url }}" defer></script>
    <script src="{{ 'global.js' | asset_url }}" defer></script>
    <script src="{{ 'split-type.min.js' | asset_url }}" defer></script>
    <script src="{{ 'gsap.min.js' | asset_url }}" defer></script>
    <script src="{{ 'scrolltrigger.min.js' | asset_url }}" defer></script>
    <script src="{{ 'animations.js' | asset_url }}" defer></script>
    {% comment %}
      GTM Head Tag
    {% endcomment %}
    <script>
      (function (w, d, s, l, i) {
        w[l] = w[l] || [];
        w[l].push({ 'gtm.start': new Date().getTime(), event: 'gtm.js' });
        var f = d.getElementsByTagName(s)[0],
          j = d.createElement(s),
          dl = l != 'dataLayer' ? '&l=' + l : '';
        j.async = true;
        j.src = 'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
        f.parentNode.insertBefore(j, f);
      })(window, document, 'script', 'dataLayer', 'GTM-5Q4CBSD');
    </script>
    {{ content_for_header }}
  </head>

  <body>
    {% comment %}
      GTM Body Tag
    {% endcomment %}

    <noscript
      ><iframe
        src="https://www.googletagmanager.com/ns.html?id=GTM-5Q4CBSD"
        height="0"
        width="0"
        style="display:none;visibility:hidden"
      ></iframe
    ></noscript>

    {% comment %}
      Skip to content accessibility link
    {% endcomment %}
    <a href="#main-content" class="skip-to-content desktop">
      <span>Skip to content</span>
    </a>

    <header>
      {% sections 'header-group' %}
    </header>

    <main id="main-content">
      {{ content_for_layout }}
    </main>

    <footer class="section-wrap">
      <div class="container">
        {% sections 'footer-group' %}
      </div>
    </footer>
    {% section 'cart-drawer' %}
  </body>
</html>
