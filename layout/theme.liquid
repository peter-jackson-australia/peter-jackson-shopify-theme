    <style>
      @media screen and (max-width: 768px) {
        #localization_form {
          display: none;
        }
      }
      
      .disclosure {
        position: relative;
        min-width: 260px;
      }
      
      #localization_form .disclosure__button {
        border: none;
        background-color: white;
        cursor: pointer;
      }
      
      #localization_form .disclosure__button-img {
      
      }
      
      #localization_form .disclosure__button-img img {
        object-fit: cover;
      }
      
      #localization_form .disclosure__button:hover {
        color: var(--neutral-700);
        border-bottom-color: var(--neutral-500);
      }
      
      #localization_form .disclosure__button[aria-expanded='true'] {
        color: var(--neutral-900);
        border-bottom-color: var(--neutral-500);
      }
      
      #localization_form .disclosure__list {
        position: absolute;
        margin-top: calc(var(--space-s) - 1px);
        background-color: var(--white);
        max-height: 300px;
        overflow-y: scroll;
        bottom: var(--space-l);
        flex-direction: column;
      }
      

      #localization_form .disclosure__list-item {
        width: 100%;
      }
      
      #localization_form .disclosure__list-item-link {
        display: block;
        padding: var(--space-xs);
        color: var(--neutral-400);
        cursor: pointer;
      }

      #localization_form .disclosure__list-item-text {
        display: flex;
        gap: var(--space-2xs);
        width: 100%;
      }
      
      #localization_form .disclosure__list-item-link[aria-current='true'] {
        color: var(--neutral-700);
      }
      
      #localization_form .disclosure__list-item-link:hover {
        color: var(--neutral-700);
        background-color: var(--neutral-50);
      }
